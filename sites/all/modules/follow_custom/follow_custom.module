<?php

function follow_custom_follow_networks_alter(&$networks, $uid = 0) {
    $networks[$uid]['instagram'] = array(
        'title' => t('Instagram'),
        'domain' => 'instagram.com',
    );

    $networks[$uid]['github'] = array(
        'title' => t('GitHub'),
        'domain' => 'github.com',
    );
}
